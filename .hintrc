{
  "extends": [
    "development"
  ],
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 115",
    "not firefox <= 115",
    "not ios_saf <= 16.5",
    "not safari <= 16.5"
  ],
  "hints": {
    "axe/aria": [
      "default",
      {
        "aria-allowed-attr": "off",
        "aria-valid-attr-value": "off",
        "aria-required-children": "off",
        "aria-roles": "off"
      }
    ],
    "no-inline-styles": "off",
    "button-type": "off",
    "axe/parsing": [
      "default",
      {
        "duplicate-id-aria": "off",
        "duplicate-id-active": "off",
        "duplicate-id": "off"
      }
    ]
  }
}